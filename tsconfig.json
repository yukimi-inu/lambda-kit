{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "declaration": false,
    "removeComments": true,
    "sourceMap": false,
    "experimentalDecorators": true,
    "strict": true,
    "outDir": "bin",
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules",
    "bin"
  ],
  "include": [
    "src/*"
  ]
}
